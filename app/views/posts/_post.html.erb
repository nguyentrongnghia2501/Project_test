<div id="<%= dom_id post %>">
  <p>
    <strong>Title:</strong>
    <%= post.title %>
  </p>

  <p>
    <strong>Content:</strong>
    <%= post.content %>
  </p>

  <p>
    <strong>User:</strong>
    <%= post.user_id %>
  </p>
  <p>
    <strong>IMG</strong>
    <p><%= image_tag post.image %></p>
  </p>
  <p>
    <strong>Status:</strong>
    <%= post.status %>
  </p>

</div>
